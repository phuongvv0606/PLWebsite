@model List<WebApplication19.Models.Menu>


<nav class="navbar navbar-inverse menuoutside" style="">

    <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
        <a style="" class="navbar-brand" href="@Url.Content("~")">
            @Html.Action("GetWebLogo","Home")
        </a>
    </div>
    <div class="collapse navbar-collapse" id="myNavbar">
        <ul class="nav navbar-nav navbar-right">
            @foreach (var item in Model)
            {
                <li>
                    <a href="@Url.Content(item.Links)">
                        @item.Names
                    </a>
                </li>
            }
        </ul>
    </div>
</nav>

<style>
    .menutop {
        list-style-type: none;
        display: inline-block;
    }

        .menutop li {
            float: left;
        }

    .navbar-brand {
        color: #000;
        font-weight: bold;
        margin-top: 5px;
        padding: 0px;
        text-transform: uppercase;
        font-style: italic;
        font-size: 40px;
        margin-left: 10px;
    }
    #myNavbar ul li a{
        color:#000;
    }
    .navbar-toggle{
        background-color:#000;
    }
    .navbar-inverse .navbar-brand {
        color:#000;
    }
    .navbar-inverse .navbar-brand:hover {
        color: #000;
    }
</style>